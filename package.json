{
  "name": "edekeestudio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "db:create:dev": "cross-env NODE_ENV=development node scripts/db/create",
    "db:create:test": "cross-env NODE_ENV=test node scripts/db/create",
    "db:delete:dev": "cross-env NODE_ENV=development node scripts/db/delete",
    "db:delete:test": "cross-env NODE_ENV=test node scripts/db/delete",
    "db:flush:dev": "cross-env NODE_ENV=development npm run db:delete:dev && npm run db:create:dev",
    "db:flush:test": "cross-env NODE_ENV=test npm run db:delete:test && npm run db:create:test",
    "migrate:dev": "npm run build && cross-env NODE_ENV=development dotenv -e .env sequelize db:migrate",
    "migrate:test": "npm run build && cross-env NODE_ENV=test dotenv -e .env sequelize db:migrate --env test",
    "migrate:prod": "npm run build && cross-env NODE_ENV=production dotenv -e .env sequelize db:migrate --env production",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start": "node dist/bin/www",
    "debug:dev": "cross-env NODE_ENV=development nodemon --inspect ",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src test --write",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier:fix": "npm run prettier -- --write",
    "prestart": "npm run build",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "testWithCoverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.6",
    "@types/nodemailer": "^6.4.6",
    "amqplib": "^0.10.2",
    "aws-sdk": "^2.1211.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "dompurify": "^2.3.10",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.5.2",
    "flutterwave-node-v3": "^1.0.11",
    "helmet": "^5.1.1",
    "jest": "^28.1.3",
    "jsdom": "^20.0.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun.js": "^8.0.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.19",
    "rand-token": "^1.0.1",
    "redis": "^4.3.0",
    "sequelize": "^6.21.4",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "slug": "^8.0.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
